   "source": [
    "with open(f\"training_data/output_gpt_1m_ToT.json\", \"r\", encoding='utf-8') as f:\n",
    "    result = json.load(f)\n",
    "\n",
    "def get_movies_length(influence_path_tmp, _x):\n",
    "    output_tmp = []\n",
    "    for _line in influence_path_tmp.split('\\n'):\n",
    "        if re.search('\\(\\d{4}\\)', _line):\n",
    "            output_tmp.append(_line)\n",
    "            _x = max(_x, len(_line))\n",
    "            if 80 > len(_line) > 70:\n",
    "                print(f\"{_line}:{len(_line)}\")\n",
    "    return _x\n",
    "\n",
    "x = 0\n",
    "for question in result:\n",
    "    x = get_movies_length(question[0], x)\n",
    "    x = get_movies_length(question[1], x)"
